{
  "extends": "fastify-tsconfig",
  "compilerOptions": {
    "outDir": "dist",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "lib": ["esnext"],
    "rootDir": "."
  },
  "include": [
    "__generated",
    "prisma/**/*.ts",
    "scraper/**/*.ts",
    "src/**/*.ts"
  ],
  "exclude": ["node_modules", "dist"]
}
